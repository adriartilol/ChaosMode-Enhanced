  <Subject Name="Dialogue_LookingForPlayer" UseSingleRandomLine="True" ShowSubtitlesIfMissing="False" SceneId="0" EffectId="-1">
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Hide and seek is over..." FileName="comments_dialogue_lookingforplayer_001_toussaintbeaufoy_001" Timestamp="2022-11-23 01:27" />
   </Line>
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Come out, come out wherever you are!" FileName="comments_dialogue_lookingforplayer_002_toussaintbeaufoy_001" Timestamp="2022-11-23 01:27" />
   </Line>
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="You can't escape, no no no no no, it's all around us... " FileName="comments_dialogue_lookingforplayer_003_toussaintbeaufoy_001" Timestamp="2022-11-23 01:27" />
   </Line>
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Come here, let me show you what I have seen. " FileName="comments_dialogue_lookingforplayer_004_toussaintbeaufoy_001" Timestamp="2022-11-23 01:27" />
   </Line>
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="These are their tunnels. How dare you trespass here. " FileName="comments_dialogue_lookingforplayer_005_toussaintbeaufoy_001" Timestamp="2022-11-23 01:27" />
   </Line>
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Vision's gone and gone's the vision. Vision's gone and gone's the vision..." FileName="comments_dialogue_lookingforplayer_006_toussaintbeaufoy_001" Timestamp="2023-02-02 01:46" />
   </Line>
