   </Line>
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="My vision's gone but my ears are true... And I hear you..." FileName="comments_dialogue_soundheard_005_toussaintbeaufoy_001" Timestamp="2023-02-02 01:46" />
   </Line>
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I heeeeear you!" FileName="comments_dialogue_soundheard_006_toussaintbeaufoy_001" Timestamp="2022-11-23 01:27" />
   </Line>
