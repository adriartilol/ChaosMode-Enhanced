   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I'm not finished with you! Come back here!" FileName="comments_dialogue_playerleavingwithdetonatorhandle_001_toussaintbeaufoy_001" Timestamp="2023-02-03 01:59" />
   </Line>
   <Line CharacterId="1" Prio="-1" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
