  <Subject Desc="Touissant reacts to a sound" Name="Dialogue_SoundHeard" UseSingleRandomLine="True" ShowSubtitlesIfMissing="False" SceneId="0" EffectId="-1">
   <Line CharacterId="1" Prio="0" TextStatus="Shippable" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Huh?!" FileName="comments_dialogue_soundheard_001_toussaintbeaufoy_001" Timestamp="2022-11-23 01:27" />
